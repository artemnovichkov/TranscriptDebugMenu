          "anchor" : "Usage",
          "level" : 2,
          "text" : "Usage",
          "type" : "heading"
        },
        {
          "code" : [
            "struct ContentView: View {",
            "    @State private var showDebugMenu = false",
            "    @State private var session = LanguageModelSession()",
            "    ",
            "    var body: some View {",
            "        Button(\"Show Debug Menu\") {",
            "            showDebugMenu = true",
            "        }",
            "        .transcriptDebugMenu(session, isPresented: $showDebugMenu)",
            "    }",
            "}"
          ],
